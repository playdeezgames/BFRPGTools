    <ProjectReference Include="..\SPLORR.HelloWorld.Assets\SPLORR.HelloWorld.Assets.vbproj" />
    <ProjectReference Include="..\SPLORR.HelloWorld.Model\SPLORR.HelloWorld.Model.vbproj" />
    <ProjectReference Include="..\SPLORR.UI\SPLORR.UI.vbproj" />
  </ItemGroup>

  <ItemGroup>
    <Import Include="SPLORR.Game" />
    <Import Include="SPLORR.HelloWorld.Assets" />
    <Import Include="SPLORR.HelloWorld.Data" />
    <Import Include="SPLORR.HelloWorld.Model" />
    <Import Include="SPLORR.UI" />
  </ItemGroup>

</Project>
